{
  "manifest_version": 3,
  "name": "es_tabs",
  "version": "1.0.0",
  "description": "An extension that solves the problem of too many tabs",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "background.js"
  }
}