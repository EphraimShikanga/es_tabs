{
  "manifest_version": 3,
  "name": "es_tabs",
  "version": "1.0.5",
  "description": "An extension that solves the problem of too many tabs",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "./src/background/background2.ts"
  }
}